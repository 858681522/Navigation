{
    "name": "graham-campbell/navigation",
    "description": "Navigation Is A Navigation Bar Generator For Laravel 4.2",
    "keywords": ["laravel", "framework", "navigation generator", "navigation", "navigation bar", "Navigation", "Laravel Navigation", "Laravel-Navigation", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/events": "~4.2",
        "illuminate/http": "~4.2",
        "illuminate/routing": "~4.2",
        "illuminate/support": "~4.2",
        "illuminate/view": "~4.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Navigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Navigation\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
